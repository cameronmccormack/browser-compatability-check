    "@types/less": "^3.0.6",
    "@types/node": "^20.10.4",
    "@types/pug": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.1",
    "immer": "^10.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-unused-exports": "^10.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@mdn/browser-compat-data": "^5.5.9",
    "chalk": "^4.1.2",
    "css-tree": "^2.3.1",
    "jsonfile": "^6.1.0",
    "less": "^4.2.0",
    "pug": "^3.0.2",
